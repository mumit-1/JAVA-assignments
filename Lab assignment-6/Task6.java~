//Task-6
import java.util.Scanner;
class Task6{
  public static void main(String [] args){
  Scanner obj = new Scanner (System.in);
    System.out.print("Enter the length of the array: ");
  int n = obj.nextInt();
  int[] array = new int[n];

  for(int i=0;i<n;i++){
   System.out.print("Enter a number: ");
   array[i] = obj.nextInt();
  }
  
  }
}